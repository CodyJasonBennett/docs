    "mdx-loader": "^3.0.2",
    "react-snap": "^1.23.0"
  },
  "dependencies": {
    "@mdx-js/react": "^1.6.22",
    "next": "^12.0.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.1.1",
    "styled-components": "^5.3.3"
  },
  "scripts": {
